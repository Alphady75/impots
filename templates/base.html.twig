<!DOCTYPE html>
<html lang="fr">


<!-- Mirrored from www.ansonika.com/magnifica/index.html by HTTrack Website Copier/3.x [XR&CO'2014], Fri, 27 May 2022 21:48:14 GMT -->
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="Magnifica Questionnaire Form Wizard includes Corona Virus Covid-19 questionnaire">
    <meta name="author" content="Ansonika">
    <title>{% block title %}{{ appName }}{% endblock %}</title>

    <!-- Favicons-->
    <link rel="shortcut icon" href="{{ asset('img/fav.png') }}" type="image/x-icon">
    <link rel="apple-touch-icon" type="image/x-icon" href="img/apple-touch-icon-57x57-precomposed.png">
    <link rel="apple-touch-icon" type="image/x-icon" sizes="72x72" href="{{ asset('themes/front/img/apple-touch-icon-72x72-precomposed.png') }}">
    <link rel="apple-touch-icon" type="image/x-icon" sizes="114x114" href="{{ asset('themes/front/img/apple-touch-icon-114x114-precomposed.png') }}">
    <link rel="apple-touch-icon" type="image/x-icon" sizes="144x144" href="{{ asset('themes/front/img/apple-touch-icon-144x144-precomposed.png') }}">

    <!-- GOOGLE WEB FONT -->
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;500;600;700&amp;display=swap" rel="stylesheet">

    <!-- BASE CSS -->
    <link href="{{ asset('themes/front/css/bootstrap.min.css') }}" rel="stylesheet">
    <link href="{{ asset('themes/front/css/menu.css') }}" rel="stylesheet">
    <link href="{{ asset('themes/front/css/style.css') }}" rel="stylesheet">
    <link href="{{ asset('themes/front/css/imports.css') }}" rel="stylesheet">
    <link href="{{ asset('themes/front/css/custom.css') }}" rel="stylesheet">
    <link href="{{ asset('themes/front/css/responsive.css') }}" rel="stylesheet">
    <link href="{{ asset('themes/front/css/vendors.css') }}" rel="stylesheet">

    <!-- Font awesome -->
    <link href="{{ asset('themes/front/css/all.css') }}" rel="stylesheet">

    <!-- YOUR CUSTOM CSS -->
    <link href="{{ asset('themes/front/css/custom.css') }}" rel="stylesheet">

    <!-- MODERNIZR MENU -->
    <script src="{{ asset('themes/front/js/modernizr.js') }}"></script>

    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}

</head>

<body class="layout_2" data-turbo="false">
    
    <div id="preloader">
        <div data-loader="circle-side"></div>
    </div><!-- /Preload -->
    
    <div id="loader_form">
        <div data-loader="circle-side-2"></div>
    </div><!-- /loader_form -->

    {% block navbar %}

        {{ include('partials/front/_navbar.html.twig') }}

        {{ include('partials/front/_mobile_menu.html.twig') }}

    {% endblock %}

    <main id="general_page">

        {{ include('partials/_flash_message.html.twig') }}

        {% block body %}{% endblock %}

    </main>

    <!-- Footer Start -->
    {% block footer %}

        {{ include('partials/front/_footer.html.twig') }}

    {% endblock %}
    <!-- Footer End --> 

    <div class="cd-overlay-nav">
        <span></span>
    </div>
    <!-- /cd-overlay-nav -->
    <div class="cd-overlay-content">
        <span></span>
    </div>
    <!-- /cd-overlay-content -->

    {% block javascripts %}

    <!-- COMMON SCRIPTS -->
    <script src="{{ asset('themes/front/js/jquery-3.6.0.min.js') }}"></script>
    <script src="{{ asset('themes/front/js/common_scripts.min.js') }}"></script>
    <script src="{{ asset('themes/front/js/velocity.min.js') }}"></script>
    <script src="{{ asset('themes/front/js/common_functions.js') }}"></script>

    <!-- Wizard script without branch -->
    <script src="{{ asset('themes/front/js/wizard_without_branch.js') }}"></script>

    {{ encore_entry_script_tags('app') }}

    <!-- SPECIFIC SCRIPTS -->
    <script src="{{ asset('themes/front/js/parallax.min.js') }}"></script>
    <script src="{{ asset('themes/front/js/owl-carousel.js') }}"></script>
    <script src="{{ asset('themes/front/js/theia-sticky-sidebar.js') }}"></script>
    <script>
    "use strict";
    $(".team-carousel").owlCarousel({
            items: 1,
            loop: false,
            margin: 10,
            autoplay: false,
            smartSpeed: 300,
            responsiveClass: false,
            responsive: {
                320: {
                    items: 1,
                },
                768: {
                    items: 2,
                },
                1000: {
                    items: 3,
                }
            }
        });
    // Faq section
    $('#faq_cat').theiaStickySidebar({
        additionalMarginTop: 100
    });
    </script>

    {% endblock %}

</body>

